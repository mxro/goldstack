{
  "name": "@mxro/server-express",
  "version": "1.0.0",
  "description": "Sample express server",
  "license": "MIT",
  "author": "Max Rohde",
  "main": "dist/serverExpress.js",
  "scripts": {
    "build": "yarn clean && yarn compile",
    "build:watch": "yarn clean && yarn compile:watch",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.json",
    "coverage": "jest --collect-coverage --passWithNoTests --config=./jest.config.js",
    "start": "yarn node dist/serverExpress.js",
    "start:watch": "nodemon --watch ../../ --ext js --exec 'yarn node dist/serverExpress.js'",
    "test": "jest --passWithNoTests --watch --config=jest.config.js",
    "test:ci": "jest --passWithNoTests --config=jest.config.js"
  },
  "dependencies": {
    "@mxro/lib-hello": "workspace:*",
    "helmet": "^3.22.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/helmet": "^0.0.47",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
